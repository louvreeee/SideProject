        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lougesite.PestDetection123",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "783d66d8-52bd-4364-b4c2-9ae3f3e2e4e3"
      }
    },
    "plugins": [
      "expo-font"

    ]
  }
